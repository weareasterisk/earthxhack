{
  "name": "earthxhack",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.14",
    "@types/react-router-dom": "^5.1.3",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-media": "^1.10.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-snap": "^1.23.0",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "run-s build:css start:code",
    "build": "run-s build:css build:code",
    "start:code": "react-scripts start",
    "build:code": "react-scripts build",
    "build:css": "tailwind build src/tailwind.scss -c tailwind.config.js -o src/generated.scss",
    "clean": "rimraf build node_modules",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "disabled:postbuild": "react-snap --no-sandbox"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "tailwindcss": "^1.1.4"
  }
}
