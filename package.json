  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.14",
    "@types/react-router-dom": "^5.1.3",
    "moment": "^2.29.4",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-facebook-pixel": "^0.1.3",
    "react-helmet": "^5.2.1",
    "react-masonry-css": "^1.0.14",
    "react-media": "^1.10.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "typescript": "~3.7.4"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development run-p build:css:watch start:code",
    "build": "cross-env NODE_ENV=production run-s build:css build:code",
    "start:code": "react-scripts start",
    "build:code": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "build:css": "postcss src/tailwind.scss -o src/generated.scss",
    "build:css:watch": "postcss src/tailwind.scss -o src/generated.scss -w",
    "clean": "rimraf build node_modules",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^9.7.3",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.10",
    "glob-all": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^7.0.26",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^1.4.1",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "purgecss-webpack-plugin": "^1.6.0",
    "rimraf": "^3.0.0",
    "tailwindcss": "^1.1.4"
  }
}
